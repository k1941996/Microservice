{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true,
    "baseUrl": ".",
    "paths": {
      "$apis/*": ["./src/apis/*"],
      "$utils/*": ["./src/utils/*"],
      "$inputComponents/*": ["./src/components/InputComponents/*"],
      "$components/*": ["./src/components/*"],
      "$authComponents/*": ["./src/components/authComponents/*"],
      "$pages/*": ["./src/pages/*"],
      "$routes/*": ["./src/routes/*"],
      "$context/*": ["./src/context/*"],
      "$config/*": ["./src/config/*"],
      "$redux/*": ["./src/Redux/*"],
      "$dispatchers/*": ["./src/Redux/Dispatchers/*"]
    }
  },
  "exclude": ["node_modules"]
}
