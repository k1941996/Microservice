{
  "compilerOptions": {
    "skipLibCheck": true,
    "noEmit": true,
    "downlevelIteration": true,
    "baseUrl": ".",
    "paths": {
      "$api/*": ["./src/api/*"],
      "$utils/*": ["./src/utils/*"],
      "$inputComponents/*": ["./src/components/InputComponents/*"],
      "$components/*": ["./src/components/*"],
      "$authComponents/*": ["./src/components/authComponents/*"],
      "$pages/*": ["./src/pages/*"],
      "$routes/*": ["./src/routes/*"],
      "$context/*": ["./src/context/*"],
      "$config/*": ["./src/config/*"],
      "$redux/*": ["./src/Redux/*"],
      "$dispatchers/*": ["./src/Redux/Dispatchers/*"]
    },
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
  },
  "exclude": ["node_modules"]
}
